generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {

  name          String @unique
  email         String @unique
  address       String
  dateofbirth   String
  age           String
  school        String
  province      String
  phone         String
  userAuthToken String @unique
  createdAt DateTime @default(now())

}
